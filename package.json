{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "description": "test coverage for [vinyl-fs (v2.4.4)](http://github.com/wearefractal/vinyl-fs) [![npm package](https://img.shields.io/npm/v/npmtest-vinyl-fs.svg?style=flat-square)](https://www.npmjs.org/package/npmtest-vinyl-fs) [![travis-ci.org build-status](https://api.travis-ci.org/npmtest/node-npmtest-vinyl-fs.svg)](https://travis-ci.org/npmtest/node-npmtest-vinyl-fs)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "utility2": "kaizhu256/node-utility2#alpha",
        "vinyl-fs": "*"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmtest/node-npmtest-vinyl-fs",
    "keywords": [
        "coverage",
        "test",
        "vinyl-fs"
    ],
    "license": "MIT",
    "main": "lib.npmtest_vinyl_fs.js",
    "name": "npmtest-vinyl-fs",
    "nameAlias": "npmtest_vinyl_fs",
    "nameOriginal": "npmtest-vinyl-fs",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmtest/node-npmtest-vinyl-fs.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.1"
}
